corr <- function(directory, threshold) {
#Content of the function
files_list <- list.files(directory, full.names = TRUE)
#creates a list of files
dat <- data.frame()
for (i in id) {
dat <- rbind(dat, read.csv(files_list[i]))
#loops through the files, rbinding them together
}
nitrate_complete <- sum(complete.cases(dat$nitrate))
sulfate_complete <- sum(complete.cases(dat$sulfate))
variable_complete <- sum(nitrate_complete, sulfate_complete)
if(varible_complete >)  {
cr <-cor(nitrate_complete, sulfate_complete)
}
else{
cr=0
}
print(cr)
}
